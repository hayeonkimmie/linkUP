<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mapper.settlelist">
    <insert id="insertSettlelist" parameterType="settlelist">
        insert settlelist (slist_id,contract_id,category_id,client_id,pname,pay,start_date,end_date,cnt)
        values (#{slist_id}, #{contract_id},#{category_id},#{client_id}, #{pname},#{pay},#{start_date},#{end_date},#{cnt})
    </insert>

</mapper>
